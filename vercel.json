{
    "version": 2,
    "builds": [
      {
        "src": "lib/main.dart",
        "use": "@vercel/flutter",
  
        "config": {
          "distDir": "build/web",
          "includeFiles": ["flutter/**"]
        }
      }
    ],
    "routes": [
      {
        "src": "/.*",
        "dest": "/index.html"
      }
    ]
  }