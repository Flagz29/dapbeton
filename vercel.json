{
    "version": 2,
    "builds": [
      {
        "src": "build/web/**",
      "use": "@vercel/static",
  
        "config": {
          "distDir": "build/web",
          "includeFiles": ["flutter/**"]
        }
      }
    ],
    "routes": [
      {
        "src": "/.*",
        "dest": "/index.html"
      }
    ]
  }